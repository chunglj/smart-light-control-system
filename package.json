{
  "name": "smart-light-control_system",
  "version": "1.0.0",
  "description": "project for building control system of smart light",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "pm2 start ecosystem.config.js && pm2 logs server",
    "restart": "pm2 restart server",
    "stop": "pm2 stop server && pm2 delete server",
    "state": "pm2 show server",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "clean": "rimraf build/",
    "produce": "npm run build && cross-env NODE_ENV=production node start.js"
  },
  "dependencies": {
    "antd": "^2.6.1",
    "koa": "2.0.0",
    "koa-bodyparser": "3.2.0",
    "koa-convert": "^1.2.0",
