{
  "name": "smart-light-control_system",
  "version": "1.0.0",
  "description": "project for building control system of smart light",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "pm2 start ecosystem.config.js --only web-server && pm2 logs web-server",
    "restart": "pm2 restart web-server && pm2 logs web-server",
    "stop": "pm2 kill",
    "status": "pm2 status",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build": "npm run clean:build && npm run build:webpack",
    "clean:build": "rimraf build/",
    "clean:logs": "rimraf logs/",
    "api": "pm2 start ecosystem.config.js --only api-server && pm2 logs api-server",
    "produce": "npm run build && cross-env NODE_ENV=production node start.js"
  },
